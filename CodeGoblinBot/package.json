{
  "name": "code-goblin",
  "version": "2.0.4",
  "description": "A bot that helps programmers make learning easier within discord.",
  "main": "./dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "concurrently \"yarn start\" \"yarn watch\"",
    "build": "rimraf tsconfig.tsbuildinfo dist/ && tsc -p tsconfig.json",
    "watch": "tsc -w",
    "watch:start": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate-changelog": "node ./scripts/clg.js"
  },
  "prettier": "@sapphire/prettier-config",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sapphire/decorators": "^6.0.0",
    "@sapphire/discord.js-utilities": "^6.0.1",
    "@sapphire/duration": "^1.0.0",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.2.0",
    "@sapphire/plugin-hmr": "^2.0.0",
    "@sapphire/plugin-i18next": "^5.0.0",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/plugin-subcommands": "^4.0.0",
    "@sapphire/plugin-utilities-store": "^1.0.0",
    "@sapphire/ratelimits": "^2.4.5",
    "@sapphire/utilities": "^3.11.0",
    "@typegoose/typegoose": "^10.0.0",
    "common-tags": "^1.8.2",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "logform": "^2.4.2",
    "mongoose": "^6.8.1",
    "ms": "^2.1.3",
    "openai": "^3.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.4",
    "@types/common-tags": "^1.8.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.17",
    "concurrently": "^7.6.0",
    "keep-a-changelog": "^2.2.1",
    "prettier": "^2.8.1",
    "rimraf": "^4.1.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.4.1"
}
